# DDS configuration
RMW_IMPLEMENTATION=rmw_fastrtps_cpp
FASTRTPS_DEFAULT_PROFILES_FILE=/udp_profile.xml

# Nav2 configuration
# Attention: MPPI controller is not compatible with **ROSbot 2 PRO**. Please use DWB or RPP controller.

CONTROLLER=mppi # options: 'dwb', 'mppi', 'rpp'
ROBOT_MODEL='' # options 'rosbot' or 'rosbot_xl'
SLAM=True # if you have a map you can run navigation without SLAM
